{
  "name": "fahimo-api-v2",
  "version": "2.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/scripts/start.js",
    "start:prod": "node dist/scripts/start.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "worker": "node dist/worker.js",
    "worker:dev": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/scripts/seed.ts",
    "seed:prod": "node dist/scripts/seed.js",
    "seed:faheemly:prod": "node dist/scripts/seed-faheemly-crawl.js",
    "smoke-test": "node dist/scripts/run-smoke-tests.js",
    "smoke-test:dev": "ts-node src/scripts/run-smoke-tests.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive:true,force:true})\"",
    "build:shared": "cd ../shared && npm install && npm run build",
    "build:widget": "cd ../widget && npm install && npm run build",
    "postbuild": "node scripts/copy-widget.js",
    "flush-cache": "node scripts/flush-cache.mjs",
    "prebuild": "npm run clean && npm run build:shared && npm run build:widget",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@fahimo/shared": "file:../shared",
    "@google/generative-ai": "^0.1.3",
    "@prisma/client": "^5.7.0",
    "@sentry/node": "^8.0.0",
    "@sentry/profiling-node": "^8.0.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "0.2.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.16.5",
    "@types/nodemailer": "^6.4.21",
    "@types/sanitize-html": "^2.13.0",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1500.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fast-levenshtein": "^3.0.0",
    "geoip-lite": "^1.4.10",
    "glob": "7.2.3",
    "groq-sdk": "0.8.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.10.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "pdf-parse": "^1.1.1",
    "redis": "^5.10.0",
    "sanitize-html": "^2.11.0",
    "socket.io": "^4.7.2",
    "twilio": "^4.19.3",
    "typescript": "^5.3.3",
    "ua-parser-js": "^1.0.37",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/geoip-lite": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "prisma": "^5.7.0",
    "rimraf": "2.7.1",
    "ts-node-dev": "^2.0.0"
  }
}
